multiplication_table = []
for i in range(1, 6):
    row = []
    for j in range(1, 6):
        row.append(i * j)
    multiplication_table.append(row)

print(multiplication_table)

# with list comprehension
multiplication_table = [[i * j for j in range(1, 6)] for i in range(1, 6)]
print(multiplication_table)
